QAAA;AACA,OAFA,MAEA,IAAA,GAAA,KAAA,GAAA,EAAA;AACA,aAAA,MAAA,GAAA,IAAA,CAAA,GAAA,CAAA,KAAA,QAAA,EAAA,KAAA,QAAA,CAAA;AACA,OAFA,MAEA,IAAA,GAAA,KAAA,uBAAA,EAAA;AACA,aAAA,MAAA,GAAA,QAAA,CAAA,KAAA,QAAA,GAAA,KAAA,QAAA,CAAA;AACA;;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,KAjBA;AAkBA,IAAA,cAlBA,0BAkBA,KAlBA,EAkBA;AACA,UAAA,SAAA,GAAA,KAAA,CAAA,MAAA,CAAA,WAAA;;AACA,UAAA,KAAA,MAAA,KAAA,GAAA,EAAA;AACA,aAAA,QAAA,GAAA,EAAA,KAAA,QAAA,GAAA,SAAA,CAAA;AACA,OAFA,MAEA,IAAA,KAAA,MAAA,KAAA,GAAA,EAAA;AACA,aAAA,QAAA,GAAA,EAAA,KAAA,QAAA,GAAA,SAAA,CAAA;AACA,OAFA,MAEA;AACA,QAAA,KAAA,CAAA,sBAAA,CAAA;AACA;AACA,KA3BA;AA4BA,IAAA,SA5BA,qBA4BA,KA5BA,EA4BA;AACA,UAAA,KAAA,MAAA,KAAA,GAAA,EAAA;AACA,aAAA,QAAA,GAAA,KAAA,QAAA,CAAA,QAAA,GAAA,KAAA,CAAA,CAAA,EAAA,CAAA,CAAA,CAAA;AACA,OAFA,MAEA,IAAA,KAAA,MAAA,KAAA,GAAA,EAAA;AACA,aAAA,QAAA,GAAA,KAAA,QAAA,CAAA,QAAA,GAAA,KAAA,CAAA,CAAA,EAAA,CAAA,CAAA,CAAA;AACA,OAFA,MAEA;AACA,QAAA,KAAA,CAAA,sBAAA,CAAA;AACA;AACA;AApCA;AAXA,CAAA","sourcesContent":["<template>\n  <div class=\"hello\">\n    <input v-model.number=\"operand1\" />\n    <input v-model.number=\"operand2\" />\n    = {{ result }}\n    <div>\n      <button @click=\"calculate\">+</button>\n      <button @click=\"calculate\">-</button>\n      <button @click=\"calculate\">*</button>\n      <button @click=\"calculate\">/</button>\n      <button @click=\"calculate\">^</button>\n      <button @click=\"calculate\">Целочисленное деление</button>\n    </div>\n    <div class=\"keyboard\">\n      <input type=\"checkbox\" id=\"checkbox\" v-model=\"checked\">\n      <label for=\"checkbox\">Отобразить экранную клавиатуру</label>\n      <div v-show=\"checked\">\n        <div class=\"radio-button\">\n          <div style=\"display: flex; justify-content: center;\">\n            <input type=\"radio\" id=\"one\" value=\"1\" v-model=\"picked\">\n            <label for=\"one\" style=\"margin-right: 20px;\">1</label>\n            <input type=\"radio\" id=\"two\" value=\"2\" v-model=\"picked\">\n            <label for=\"two\">2</label>\n          </div>\n          <span>Выбрано поле № {{ picked }}</span>\n        </div>\n        <button\n        v-for=\"keyButton in keyButtons\"\n        :key=\"keyButton\"\n        @click=\"inputNumberKey\"\n        >{{ keyButton }}</button>\n        <button @click=\"backSpace\">BackSpace</button>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  data: () => ({\n    operand1: 0,\n    operand2: 0,\n    result: 0,\n    keyButtons: ['1', '2', '3', '4', '5', '6', '7', '8', '9', '0'],\n    // keyButtons: ['1', '2', '3', '4', '5', '6', '7', '8', '9', '0', 'BackSpace'], - расскажите, как в этом случае сделать, голову сломал\n    checked: false,\n    picked: ''\n  }),\n  props: {},\n  methods: {\n    calculate (event) {\n      const act = event.target.textContent\n      if (act === '+') {\n        this.result = this.operand1 + this.operand2\n      } else if (act === '-') {\n        this.result = this.operand1 - this.operand2\n      } else if (act === '/') {\n        this.result = this.operand1 / this.operand2\n      } else if (act === '*') {\n        this.result = this.operand1 * this.operand2\n      } else if (act === '^') {\n        this.result = Math.pow(this.operand1, this.operand2)\n      } else if (act === 'Целочисленное деление') {\n        this.result = parseInt(this.operand1 / this.operand2)\n      }\n      console.log(act)\n    },\n    inputNumberKey (event) {\n      const numberKey = event.target.textContent\n      if (this.picked === '1') {\n        this.operand1 = +(this.operand1 + numberKey)\n      } else if (this.picked === '2') {\n        this.operand2 = +(this.operand2 + numberKey)\n      } else {\n        alert('Выберете поле ввода!')\n      }\n    },\n    backSpace (event) {\n      if (this.picked === '1') {\n        this.operand1 = this.operand1.toString().slice(0, -1)\n      } else if (this.picked === '2') {\n        this.operand2 = this.operand2.toString().slice(0, -1)\n      } else {\n        alert('Выберете поле ввода!')\n      }\n    }\n  }\n}\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped lang=\"scss\">\nh3 {\n  margin: 40px 0 0;\n}\nul {\n  list-style-type: none;\n  padding: 0;\n}\nli {\n  display: inline-block;\n  margin: 0 10px;\n}\na {\n  color: #42b983;\n}\n</style>\n"],"sourceRoot":"src/components"}]}